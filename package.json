{
  "name": "questionanswertranslator",
  "version": "1.0.0",
  "scripts": {
    "cosmosdb:import": "ts-node cosmosdb/src/import.ts",
    "functions:build": "tsc",
    "functions:start": "func start",
    "local:create": "docker-compose up",
    "local:cosmosdbImport": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node cosmosdb/src/import.ts",
    "local:destroy": "docker-compose down",
    "local:functionsUpdate": "docker-compose up -d --no-deps --build localfunctions",
    "local:reactUpdate": "docker-compose up -d --no-deps --build localreact",
    "react:build": "react-scripts build",
    "react:start": "FORCE_COLOR=true react-scripts start | cat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infhyroyage/QuestionAnswerTranslator.git"
  },
  "license": "MIT",
  "dependencies": {
    "@azure/cosmos": "^3.17.1",
    "@azure/functions": "^3.2.0",
    "@azure/identity": "^3.0.0",
    "@azure/keyvault-keys": "^4.5.0",
    "@azure/keyvault-secrets": "^4.5.1",
    "@azure/msal-browser": "^2.28.3",
    "@azure/msal-react": "^1.4.7",
    "axios": "^0.27.2",
    "deepcopy": "^2.1.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "recoil": "^0.7.6",
    "uuid": "^9.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "react-scripts": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
