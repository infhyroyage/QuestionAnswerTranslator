{
  "name": "questionanswertranslator",
  "version": "1.0.0",
  "scripts": {
    "cosmosdb:init": "ts-node cosmosdb/src/initialImport.ts",
    "cosmosdb:manual": "ts-node cosmosdb/src/manualImport.ts",
    "cosmosdb:localInit": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node cosmosdb/src/initialImport.ts",
    "cosmosdb:localManual": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node cosmosdb/src/manualImport.ts",
    "functions:start": "cd functions; tsc; func start",
    "functions:localStart": "docker-compose up --build",
    "functions:localUpdate": "docker-compose up -d --no-deps --build localfunctions",
    "functions:localDestroy": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infhyroyage/QuestionAnswerTranslator.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infhyroyage/QuestionAnswerTranslator/issues"
  },
  "homepage": "https://github.com/infhyroyage/QuestionAnswerTranslator#readme",
  "devDependencies": {
    "@types/node": "^18.7.18",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.1",
    "@azure/functions": "^3.2.0",
    "@azure/identity": "^3.0.0",
    "@azure/keyvault-keys": "^4.5.0",
    "@azure/keyvault-secrets": "^4.5.1",
    "deepcopy": "^2.1.0",
    "uuid": "^9.0.0"
  }
}
